# Environment Variables for AlumniVerse
# Copy this file to .env and fill in your actual values

# Google OAuth Configuration
VITE_APP_GOOGLE_KEY=your_google_oauth_client_id_here

# API Configuration (if you have a backend)
VITE_APP_API_URL=https://your-api-domain.com/api

# App Configuration
VITE_APP_NAME=AlumniVerse
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=LinkedIn-style Alumni Network Platform

# Development/Production Mode
VITE_NODE_ENV=development
